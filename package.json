  },
  "homepage": "https://github.com/Zhouzi/TheaterJS",
  "devDependencies": {
    "@babel/cli": "7.6.0",
    "@babel/core": "7.6.0",
    "@babel/preset-env": "7.6.0",
    "babel-jest": "24.9.0",
    "jest": "24.9.0",
    "prettier": "1.18.2"
  }
